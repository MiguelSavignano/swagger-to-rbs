
# Classes

class <%= @module_name %>
  include HTTParty

  def initialize: -> void
  <%- @data[:endpoints].each do |endpoint| -%>

  <%- if endpoint[:method] == 'get' -%>
  def <%= endpoint[:method_name] %>(<%= endpoint[:parameters_typed] %>) -> HTTParty::Response
  <%- else -%>
  def <%= endpoint[:method_name] %>(<%= endpoint[:body_typed] %>) -> HTTParty::Response
  <%- end -%>
  <%- end -%>
end
